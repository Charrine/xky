<UserControl x:Class="MyDemo.UserControl.Controls.TransferDemoCtl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Background="{DynamicResource RegionBrush}"
             xmlns:controls="clr-namespace:Xky.UI.Controls;assembly=Xky.UI"
             xmlns:other="clr-namespace:Xky.UI.Controls.Other;assembly=Xky.UI"
             xmlns:transfer="clr-namespace:Xky.UI.Controls.Transfer;assembly=Xky.UI">
    <other:TransitioningContentControl>
        <transfer:Transfer ItemsSource="{Binding DataList}" Margin="32" Height="300">
            <transfer:Transfer.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Name}"/>
                </DataTemplate>
            </transfer:Transfer.ItemTemplate>
        </transfer:Transfer>
    </other:TransitioningContentControl>
</UserControl>
