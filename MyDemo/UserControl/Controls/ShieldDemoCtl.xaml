        <StackPanel Orientation="Horizontal" Margin="32">
            <other:Shield Subject=".net" Status=">=4.0" Color="#1182c3"/>
            <other:Shield Subject="c#" Status="7.0" Margin="4,0,0,0" Color="#1182c3"/>
