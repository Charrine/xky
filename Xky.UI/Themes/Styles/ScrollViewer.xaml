<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:Xky.UI.Controls"
                    xmlns:other="clr-namespace:Xky.UI.Controls.Other">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Base/ScrollViewerBaseStyle.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!--default style-->
    <Style BasedOn="{StaticResource ScrollViewerNativeBaseStyle}" TargetType="ScrollViewer"/>

    <Style BasedOn="{StaticResource ScrollViewerBaseStyle}" TargetType="other:ScrollViewer"/>

</ResourceDictionary>